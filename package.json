{
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "start": "webpack-dev-server --host 0.0.0.0 --progress --colors --port 3000",
    "lint": "eslint ./src --ext .js,.ts",
    "lint:fix": "eslint ./src --ext .js,.ts --fix",
    "clean": "shx rm -rf dist",
    "build": "cross-env NODE_ENV=production webpack",
    "builddev": "cross-env NODE_ENV=development webpack",
    "build:stats": "cross-env NODE_ENV=production webpack --json > stats.json",
    "copydeploy": "shx cp -r assets dist",
    "predeploydev": "npm run builddev && npm run copydeploy",
    "predeploy": "npm run build && npm run copydeploy",
    "serve": "ws --http2 -d dist",
    "trydeploy": "npm run predeploydev && npm run serve",
    "deploy": "npm run predeploydev && netlify deploy --dir=dist",
    "deploy-prod": "npm run predeploy && netlify deploy --dir=dist -p"
  },
  "dependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-typescript": "^7.8.3",
    "@types/aframe": "^0.8.4",
    "aframe": "^1.0.4",
    "aframe-extras": "^6.0.1",
    "aframe-proxy-event-component": "^2.1.0",
    "aframe-render-order-component": "^1.1.0",
    "aframe-slice9-component": "^1.0.0",
    "aframe-state-component": "^6.8.0",
    "aframe-super-hot-html-loader": "*",
    "aframe-super-hot-loader": "*",
    "babel-loader": "^8.0.5",
    "cross-env": "4.0.0",
    "css-loader": "^0.28.7",
    "debug": "^4.1.0",
    "diff-dom": "^3.1.0",
    "fork-ts-checker-webpack-plugin": "^1.5.1",
    "html-entities": "^1.2.1",
    "html-require-loader": "*",
    "ip": "1.1.5",
    "json-loader": "^0.5.7",
    "style-loader": "^0.23.1",
    "super-nunjucks-loader": "*",
    "three": "^0.111.0",
    "three-actor": "^0.0.3",
    "ts-loader": "^6.2.0",
    "typescript": "^3.6.4",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.5",
    "webpack-cli": "3.2.3",
    "webpack-dev-server": "^3.2.0",
    "webpack-glsl-loader": "^1.0.1",
    "webpack-sources": "1.0.1"
  },
  "devDependencies": {
    "@types/webpack-env": "^1.14.1",
    "@typescript-eslint/eslint-plugin": "^2.4.0",
    "@typescript-eslint/parser": "^2.4.0",
    "babel-eslint": "^10.0.3",
    "clean-webpack-plugin": "^3.0.0",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-prettier": "^3.1.1",
    "gh-pages": "^2.1.1",
    "html-webpack-plugin": "^3.2.0",
    "local-web-server": "^4.0.0",
    "prettier": "^1.18.2",
    "workbox-webpack-plugin": "^5.0.0"
  }
}
